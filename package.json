    "serve:site": "http-server dist -c-1",
    "build": "sass wireframe/sass/main.scss wireframe/css/main.css",
    "build:site": "sass src/assets/scss/main.scss dist/assets/css/main.css && node scripts/build-site.js && mkdir -p dist/assets/webfonts && cp -r node_modules/@fortawesome/fontawesome-free/webfonts/* dist/assets/webfonts/",
